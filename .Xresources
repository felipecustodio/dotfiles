! rofi
rofi.font:             SF Mono 12

! URxvt
URxvt.scrollBar: false
URxvt.font: xft:Range Mono:size=10

! X colors.
! Generated by 'wal'
*foreground:        #c5c8c9
*background:        #1a2427
*.foreground:       #c5c8c9
*.background:       #1a2427
emacs*foreground:   #c5c8c9
emacs*background:   #1a2427
URxvt*foreground:   #c5c8c9
XTerm*foreground:   #c5c8c9
UXTerm*foreground:  #c5c8c9
URxvt*background:   [100]#1a2427
XTerm*background:   #1a2427
UXTerm*background:  #1a2427
URxvt*cursorColor:  #c5c8c9
XTerm*cursorColor:  #c5c8c9
UXTerm*cursorColor: #c5c8c9
URxvt*borderColor:  [100]#1a2427

! Colors 0-15.
*.color0: #1a2427
*color0:  #1a2427
*.color1: #922d2a
*color1:  #922d2a
*.color2: #396f7d
*color2:  #396f7d
*.color3: #718c8e
*color3:  #718c8e
*.color4: #73a2af
*color4:  #73a2af
*.color5: #d38681
*color5:  #d38681
*.color6: #8fb6bf
*color6:  #8fb6bf
*.color7: #c5c8c9
*color7:  #c5c8c9
*.color8: #535a5d
*color8:  #535a5d
*.color9: #922d2a
*color9:  #922d2a
*.color10: #396f7d
*color10:  #396f7d
*.color11: #718c8e
*color11:  #718c8e
*.color12: #73a2af
*color12:  #73a2af
*.color13: #d38681
*color13:  #d38681
*.color14: #8fb6bf
*color14:  #8fb6bf
*.color15: #c5c8c9
*color15:  #c5c8c9

! Black color that will not be affected by bold highlighting.
*.color66: #1a2427
*color66:  #1a2427

! Xclock colors.
XClock*foreground: #c5c8c9
XClock*background: #1a2427
XClock*majorColor:  rgba:c5/c8/c9/ff
XClock*minorColor:  rgba:c5/c8/c9/ff
XClock*hourColor:   rgba:c5/c8/c9/ff
XClock*minuteColor: rgba:c5/c8/c9/ff
XClock*secondColor: rgba:c5/c8/c9/ff

! Set depth to make transparency work.
URxvt*depth: 32


