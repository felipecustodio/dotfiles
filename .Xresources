! Black color that will not be affected by bold highlighting.
*.color66: #1f1915
*color66:  #1f1915

! Xclock colors.
XClock*foreground: #c7c5c4
XClock*background: #1f1915
XClock*majorColor:  rgba:c7/c5/c4/ff
XClock*minorColor:  rgba:c7/c5/c4/ff
XClock*hourColor:   rgba:c7/c5/c4/ff
XClock*minuteColor: rgba:c7/c5/c4/ff
XClock*secondColor: rgba:c7/c5/c4/ff

! Set depth to make transparency work.
URxvt*depth: 32

